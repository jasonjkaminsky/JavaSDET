package com.syntax.class26;

import java.util.ArrayList;

public class Demo1 {

    public static void main(String[] args) {
        ArrayList<Integer> numbers = new ArrayList();
        
        for (int i = 0; i < 10; i++) {
            numbers.add(i);
            System.out.println(numbers); // print entire ArrayList
        }

        for (int i = 0; i < numbers.size(); i++){
            System.out.println(numbers.get(i)); // print each element on separate line
        }
    }
}
